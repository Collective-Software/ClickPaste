<?xml version="1.0" encoding="utf-8"?>
<Weavers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="FodyWeavers.xsd">
  <Costura>
    <!-- Embed native AutoIt DLLs -->
    <Unmanaged32Assemblies>
      AutoItX3
    </Unmanaged32Assemblies>
    <Unmanaged64Assemblies>
      AutoItX3_x64
    </Unmanaged64Assemblies>
    <!-- Don't embed debug symbols or XML docs -->
    <IncludeDebugSymbols>false</IncludeDebugSymbols>
    <ExcludeAssemblies>
      AutoItX3.Assembly.xml
    </ExcludeAssemblies>
    <!-- Delete embedded DLLs from output -->
    <DisableCleanup>false</DisableCleanup>
    <DeleteReferences>true</DeleteReferences>
  </Costura>
</Weavers>
